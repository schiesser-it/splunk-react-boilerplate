{
  "name": "splunk-react-mui-boilerplate",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "clean": "rm -fr stage",
    "build": "yarn run clean && NODE_ENV=production webpack --bail",
    "start": "webpack --watch",
    "lint": "eslint src --ext \".js,.jsx\"",
    "lint:fix": "eslint src --ext \".js, .jsx\" --fix",
    "format": "git ls-files | grep -E \"\\.(jsx?|css)$\" | xargs prettier --write",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.13.4",
    "@mui/x-date-pickers": "^6.7.0",
    "@splunk/dashboard-context": "^23.3.0",
    "@splunk/dashboard-core": "^23.3.0",
    "@splunk/dashboard-presets": "^23.3.0",
    "@splunk/react-ui": "^4.4.0",
    "@splunk/splunk-utils": "^2.3.4",
    "@splunk/themes": "^0.8.0",
    "@splunk/visualization-context": "^25.7.2",
    "dayjs": "^1.11.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "scriptjs": "^2.5.8",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@splunk/webpack-configs": "^5.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.4",
    "css-loader": "5.2.7",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-webpack": "^0.13.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "jest-junit": "^10.0.0",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.12.0",
    "style-loader": "2.0.0",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.3"
  },
  "engines": {
    "node": ">=16"
  }
}
